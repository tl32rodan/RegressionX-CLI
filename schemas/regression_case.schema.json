{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://regressionx.dev/schemas/regression_case.schema.json",
  "title": "RegressionX Regression Case",
  "type": "object",
  "required": ["case_id", "params"],
  "additionalProperties": false,
  "properties": {
    "case_id": {
      "type": "string",
      "minLength": 1
    },
    "params": {
      "type": "object",
      "additionalProperties": true
    },
    "metadata": {
      "type": "object",
      "additionalProperties": true
    },
    "extra_ignore": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    }
  }
}
